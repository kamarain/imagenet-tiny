% Run first this to know where development kit is installed
devkit_conf
dkconf = conf;
clear conf;


conf.selected_synsets = ...
    {'n02676566','n01773157','n01622779','n03452741','n01944390'};
conf.dataName = 'ImageNet-tiny-5'
conf.meta_file = ...
    fullfile(dkconf.devkitDir,'data/meta_clsloc.mat');
conf.val_ground_truth_file = ...
    fullfile(dkconf.devkitDir,...
             'data/ILSVRC2014_clsloc_validation_ground_truth.txt');
conf.val_blacklist_file = ...
    fullfile(dkconf.devkitDir,...
             'data/ILSVRC2014_clsloc_validation_blacklist.txt');
conf.new_val_ground_truth_file = ...
    fullfile(['ILSVRC2014_clsloc_validation_ground_truth-' ...
              conf.dataName '.txt']);
conf.new_val_blacklist_file = ...
    fullfile(['ILSVRC2014_clsloc_validation_blacklist-' ...
              conf.dataName '.txt']);
conf.new_val_list = ...
    fullfile(['ILSVRC2014_filelist-' ...
              conf.dataName '.txt']);
